<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="#949BA0" />
            <stroke
                android:width="1dp"
                android:color="#013220" />
        </shape>
    </item>
    <item
        android:id="@android:id/progress"
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <clip>
            <scale android:scaleWidth="100%">
                <bitmap
                    android:dither="true"
                    android:src="@drawable/heal_bar" />
            </scale>
        </clip>
    </item>
</layer-list>

